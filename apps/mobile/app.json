{
    "expo": {
        "name": "HinaNavi Mobile",
        "slug": "hinanavi-mobile",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "splash": {
            "image": "./assets/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#1a1a2e"
        },
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "jp.evac.hinanavi"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#1a1a2e"
            },
            "package": "jp.evac.hinanavi"
        },
        "extra": {
            "apiBaseUrl": "http://localhost:3000"
        }
    }
}